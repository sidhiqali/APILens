{
  "name": "apilens",
  "displayName": "APILens",
  "description": "API monitoring and management directly in VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "api",
    "monitoring",
    "swagger",
    "openapi",
    "rest"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "apilens.openDashboard",
        "title": "Open Dashboard",
        "category": "APILens",
        "icon": "$(dashboard)"
      },
      {
        "command": "apilens.addAPI",
        "title": "Add API",
        "category": "APILens",
        "icon": "$(add)"
      },
      {
        "command": "apilens.viewNotifications",
        "title": "View Notifications",
        "category": "APILens",
        "icon": "$(bell)"
      },
      {
        "command": "apilens.openAnalytics",
        "title": "Open Analytics",
        "category": "APILens",
        "icon": "$(graph)"
      },
      {
        "command": "apilens.refreshAPIs",
        "title": "Refresh APIs",
        "category": "APILens",
        "icon": "$(refresh)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "apilens-explorer",
          "title": "APILens",
          "icon": "$(cloud)"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "apilens.openDashboard"
        },
        {
          "command": "apilens.addAPI"
        },
        {
          "command": "apilens.viewNotifications"
        },
        {
          "command": "apilens.openAnalytics"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  }
}
